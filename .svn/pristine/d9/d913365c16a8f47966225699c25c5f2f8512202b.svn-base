		/// 治疗效果  （用选项：治愈、好转、维持、进展）
		/// </summary>
		public string TreatResult
		{
			set{ _treatresult=value;}
			get{return _treatresult;}
		}
		/// <summary>
		/// 如果为慢性病需要长期服药的（用药史）
		/// </summary>
		public string MedicateHistory
		{
			set{ _medicatehistory=value;}
			get{return _medicatehistory;}
		}
