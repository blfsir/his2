		/// 疾病诊断
		/// </summary>
		public string Disease
		{
			set{ _disease=value;}
			get{return _disease;}
		}
		/// <summary>
		/// 发病年份
		/// </summary>
		public string IllYear
		{
			set{ _illyear=value;}
			get{return _illyear;}
		}
