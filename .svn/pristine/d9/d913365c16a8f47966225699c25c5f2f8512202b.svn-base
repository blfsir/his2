		{}
		#region Model
		private int _id;
		private int? _pid=0;
		private int? _gid=0;
		private string _disease;
		private string _illyear;
		private string _treatinfo;
		private string _treatresult;
		private string _medicatehistory;
		/// <summary>
		/// 
		/// </summary>
		public int ID
		{
			set{ _id=value;}
			get{return _id;}
		}
		/// <summary>
		/// 病人ID
		/// </summary>
		public int? PID
		{
			set{ _pid=value;}
			get{return _pid;}
		}
		/// <summary>
		/// 一般情况ID
		/// </summary>
		public int? GID
		{
			set{ _gid=value;}
			get{return _gid;}
		}
